{
  "LoadBalancerName": "conta-med-alb",
  "Scheme": "internet-facing",
  "Type": "application",
  "IpAddressType": "ipv4",
  "Subnets": [
    "subnet-091648750fb783d11",
    "subnet-075a1c2e9fd0f9f8f"
  ],
  "SecurityGroups": [
    "sg-032d8f3a86b5a9b7c"
  ],
  "Tags": [
    {
      "Key": "Name",
      "Value": "conta-med-alb"
    },
    {
      "Key": "Environment",
      "Value": "production"
    }
  ],
  "TargetGroup": {
    "Name": "conta-med-targets",
    "Protocol": "HTTP",
    "Port": 8080,
    "TargetType": "ip",
    "HealthCheckPath": "/ping",
    "HealthCheckIntervalSeconds": 30,
    "HealthCheckTimeoutSeconds": 5,
    "HealthyThresholdCount": 2,
    "UnhealthyThresholdCount": 3
  },
  "Listeners": [
    {
      "Port": 80,
      "Protocol": "HTTP",
      "DefaultActions": [
        {
          "Type": "redirect",
          "RedirectConfig": {
            "Protocol": "HTTPS",
            "Port": "443",
            "StatusCode": "HTTP_301"
          }
        }
      ]
    },
    {
      "Port": 443,
      "Protocol": "HTTPS",
      "CertificateArn": "arn:aws:acm:us-east-1:323195246434:certificate/YOUR_CERT_ARN",
      "DefaultActions": [
        {
          "Type": "forward",
          "TargetGroupArn": "TARGET_GROUP_ARN"
        }
      ]
    }
  ]
} 